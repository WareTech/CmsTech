<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  	<display-name>CmsTech</display-name>
	<servlet>
    	<servlet-name>StartupServlet</servlet-name>
    	<servlet-class>com.WareTech.CmsTech.servlets.StartupServlet</servlet-class>
    	<load-on-startup>1</load-on-startup>
    </servlet> 
    <filter>
        <filter-name>TransactionFilter</filter-name>
        <filter-class>com.WareTech.CmsTech.filter.TransactionFilter</filter-class>
	</filter>
    <filter>
        <filter-name>SecurityFilter</filter-name>
        <filter-class>com.WareTech.CmsTech.filter.SecurityFilter</filter-class>
	</filter>
	<filter-mapping>
        <filter-name>TransactionFilter</filter-name>                        
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
	<filter-mapping>
        <filter-name>SecurityFilter</filter-name>                        
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>